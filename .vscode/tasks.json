{
    "tasks": [
        {
            "type": "cppbuild",
            "label": "C/C++: gcc build active file",
            "command": "/usr/lib64/ccache/gcc",
            "args": [
                "-fdiagnostics-color=always",
                "-lm",
                "-g",
                "${file}",
                "Sales.c",
                "GroupMonthByRevenue.c",
                "GroupProductByRevenue.c",
                "GroupCityByRevenue.c",
                "GroupCategoryByRevenue.c",
                "GroupUtil.c",
                "-o",
                "${fileDirname}/${fileBasenameNoExtension}"
            ],
            "options": {
                "cwd": "${fileDirname}"
            },
            "problemMatcher": [
                "$gcc"
            ],
            "group": {
                "kind": "build",
                "isDefault": true
            },
            "detail": "Task generated by Debugger."
        }
    ],
    "version": "2.0.0"
}